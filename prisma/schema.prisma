generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("NEON_DATABASE_URL")
}

model users {
  id             String   @id @default(uuid()) @db.Uuid
  wallet_address String   @unique
  nonce          String
  verified_at    DateTime? @db.Timestamptz
  created_at     DateTime  @default(now()) @db.Timestamptz
}


